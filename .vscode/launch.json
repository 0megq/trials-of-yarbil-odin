{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "cppvsdbg",
			"request": "launch",
			"preLaunchTask": "Debug Build",
			"name": "Debug",
			"program": "${workspaceFolder}/build/build.exe",
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "f5anything",
			"request": "launch",
			"name": "Run",
			"command": "odin run . -debug -out:build/build.exe -vet -strict-style",
		},
		{
			"type": "f5anything",
			"request": "launch",
			"name": "Release",
			"command": "odin run . -out:build/yarbil.exe -vet -strict-style -subsystem:windows",
		},
		{
			"type": "f5anything",
			"request": "launch",
			"name": "Test",
			"command": ".\test.bat",
		}
	]
}